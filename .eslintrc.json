{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["prettier", "airbnb-base"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "quotes": ["error", "double", { "avoidEscape": true }],
    "no-param-reassign": "off",
    "no-plusplus": "off",
    "no-use-before-define": "off",
    "object-curly-newline": "off",
    "import/no-cycle": "off"
  }
}
